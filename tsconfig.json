{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@models/*": ["./models/*"],
    },
    "traceResolution": false,
    "noEmit": false,
    "outDir": "./dist/",
    "sourceMap": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["./**/*.ts", "./**/*.tsx"],
  "types": [
    "node"
  ]
}